<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22" fx:controller="exemplo.trabalho1.controller.estudante.EstudanteController">
    <children>
        <Label fx:id="lblNomeTela" alignment="CENTER_RIGHT" text="Incluir Estudante">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label fx:id="lblNomeEstudante" />
        <Label text="Nome">
            <font>
                <Font size="15.0" />
            </font>
            <padding>
                <Insets bottom="10.0" />
            </padding>
        </Label>
        <TextField fx:id="txtNomEst" prefHeight="25.0" prefWidth="547.0" />
        <HBox prefHeight="37.0" prefWidth="600.0" spacing="10.0">
            <children>
                <Button fx:id="btnConf" onAction="#btnInserir" prefHeight="37.0" prefWidth="74.0" text="Confirmar">
                    <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                    </padding>
                </Button>
                <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#btnVoltarOnAction" prefHeight="37.0" prefWidth="74.0" text="Voltar" />
            </children>
            <padding>
                <Insets bottom="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="30.0" />
    </padding>
</VBox>